{
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/okinawa-lagoon.jp/index.html"
    },
    {
      "source": "/okinawa-lagoon.jp/(.*)",
      "destination": "/okinawa-lagoon.jp/$1"
    }
  ]
}

